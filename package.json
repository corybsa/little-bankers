    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@angular/service-worker": "^17.0.6",
    "@auth0/angular-jwt": "^5.1.2",
    "@aws-sdk/client-rekognition": "^3.382.0",
    "@aws-sdk/client-ses": "^3.382.0",
    "@aws-sdk/client-sns": "^3.382.0",
    "@aws-sdk/client-sqs": "^3.382.0",
    "@ericblade/quagga2": "^1.8.2",
    "@ngrx/effects": "^17.0.1",
    "@ngrx/store": "^17.0.1",
    "@ngrx/store-devtools": "^17.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "canvas-confetti": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "~16.0.0",
    "easy-speech": "^2.1.0",
