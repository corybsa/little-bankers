{
  "name": "little-bankers",
  "version": "0.0.0",
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run dev\" \"npm run dev-server\"",
    "build": "ng build -c prod",
    "build-dev": "ng build -c dev --aot",
    "build-prod": "ng build -c prod --aot --build-optimizer --optimization",
    "build-server": "tsc -p tsconfig.server.json",
    "watch": "ng build --watch -c dev",
    "test": "ng test",
    "dev": "ng build -c dev --watch --aot",
    "dev-server": "tsc-watch -p tsconfig.server.json --onFirstSuccess \"npm run copy-files\" --onSuccess \"node dist-server/server.js\"",
    "copy-files": "npm run copy-config && npm run copy-email-templates",
    "copy-config": "xcopy /Y .\\server\\config\\.env .\\dist-server\\config\\",
    "copy-email-templates": "xcopy /E/Y .\\server\\email-templates\\* .\\dist-server\\email-templates\\",
    "stripe-listener": "~/Downloads/stripe.exe listen --forward-to localhost:8081/stripehook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/material": "^17.0.3",
